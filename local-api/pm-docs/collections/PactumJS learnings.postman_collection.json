                "exec": [
                  "pm.test(\"testing the POST a post operation\",()=>{\r",
                  "    pm.expect(pm.response.code).to.eql(201)\r",
                  "    const requestData = pm.request.body.raw;\r",
                  "    const reqJson = JSON.parse(requestData)\r",
                  "    const resJson = pm.response.json()\r",
                  "\r",
                  "    pm.expect(resJson.title).to.eql(reqJson.title);\r",
                  "    pm.expect(resJson.body).to.eql(reqJson.body);\r",
                  "    pm.expect(resJson.userId).to.eql(reqJson.userId);\r",
                  "\r",
                  "    console.log(\"Generated id : \", resJson.id)\r",
                  "    \r",
                  "})"
