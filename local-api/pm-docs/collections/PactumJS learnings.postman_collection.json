        {
          "name": "data matching",
          "item": [
            {
              "name": "Post a post",
              "event": [
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "pm.test(\"status code - POST operation\", () => {\r",
                      "    pm.response.to.have.status(201);\r",
                      "})\r",
                      "\r",
                      "const res = pm.response.json()\r",
                      "\r",
                      "// Store postId\r",
                      "pm.collectionVariables.set(\"postId\", res.id);\r",
                      "console.log(res.id)\r",
                      "\r",
                      "pm.test(\"data matching - POST operation\",()=>{\r",
                      "    pm.expect(res.id).to.be.a(\"string\");\r",
                      "    pm.expect(res.title).to.be.a(\"string\");\r",
                      "    pm.expect(res.body).to.be.a(\"string\");\r",
                      "})\r",
                      ""
                    ],
                    "type": "text/javascript",
                    "packages": {},
                    "requests": {}
                  }
                }
              ],
              "request": {
                "method": "POST",
                "header": [],
                "body": {
                  "mode": "raw",
                  "raw": "{\r\n  \"title\": \"My Test Post\",\r\n  \"body\": \"This is a dynamic post body\"\r\n}\r\n",
                  "options": {
                    "raw": {
                      "language": "json"
                    }
                  }
                },
